{
    "name": "utchattanooga/utccloud",
    "description": "Project template for Thunder projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Chris Gilligan",
            "email": "chris-gilligan@utc.edu"
        }
    ],
    "repositories": {
        "lenient": {
            "type": "composer",
            "url": "https://packages.drupal.org/lenient"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "php": ">=8.1",
        "acquia/blt": "^13.7",
        "acquia/blt-travis": "^1.1",
        "bower-asset/jquery-simple-color": "^1.2",
        "composer/installers": "^1.2",
        "drupal/acquia_connector": "^4.0",
        "drupal/acquia_purge": "^1.3",
        "drupal/aggregator": "^2.0",
        "drupal/allowed_formats": "^2.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/big_pipe_sessionless": "^2.1",
        "drupal/bigmenu": "^2.0@RC",
        "drupal/block_visibility_groups": "^2.0",
        "drupal/cas": "^2.1",
        "drupal/cas_attributes": "^2.0@beta",
        "drupal/ckeditor": "^1.0",
        "drupal/ckeditor_accordion": "^2.0",
        "drupal/ckeditor_bootstrap_buttons": "^2.0@dev",
        "drupal/ckeditor_codemirror": "^3.0@beta",
        "drupal/color": "^1.0",
        "drupal/color_field": "^3.0",
        "drupal/components": "^3.0@beta",
        "drupal/composer_deploy": "^1.7",
        "drupal/config_filter": "^2.4",
        "drupal/config_ignore": "^2.4",
        "drupal/config_split": "^2.0@RC",
        "drupal/context": "^5.0@RC",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/cshs": "^4.0",
        "drupal/entity": "^1.4",
        "drupal/entity_browser": "^2.9",
        "drupal/entity_browser_block": "^1.3",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/entity_embed": "^1.4",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/entityqueue": "^1.6",
        "drupal/environment_indicator": "^4.0",
        "drupal/exif_orientation": "^1.2",
        "drupal/field_formatter": "^3.0@RC",
        "drupal/field_permissions": "^1.2",
        "drupal/fontawesome": "^2.25",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/google_cse": "^3.8",
        "drupal/google_tag": "^2.0",
        "drupal/gtranslate": "^3.0",
        "drupal/honeypot": "^2.1",
        "drupal/layout_builder_modal": "^1.2",
        "drupal/layout_builder_operation_link": "^2.1",
        "drupal/layout_builder_restrictions": "^2.18",
        "drupal/layout_builder_styles": "^2.0",
        "drupal/layout_library": "^1.0@beta",
        "drupal/libraries": "^4.0",
        "drupal/masquerade": "^2.0@RC",
        "drupal/maxlength": "^2.1",
        "drupal/media_entity_file_redirect": "^1.0",
        "drupal/memcache": "^2.5",
        "drupal/menu_block": "^1.10",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/moderated_content_bulk_publish": "^2.0",
        "drupal/module_filter": "^4.0",
        "drupal/multiple_fields_remove_button": "^2.2",
        "drupal/office_hours": "^1.11",
        "drupal/paragraph_view_mode": "^3.1",
        "drupal/realname": "^2.0@beta",
        "drupal/responsive_tables_filter": "^1.17",
        "drupal/seckit": "^2.0",
        "drupal/site_alert": "^1.3",
        "drupal/sitemap": "^2.0@beta",
        "drupal/slick_views": "^2.7",
        "drupal/smart_trim": "^2.1",
        "drupal/social_media_links": "^2.9",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/svg_image": "^3.0",
        "drupal/tablefield": "^2.4",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/twitter_block": "^3.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/views_accordion": "^2.0",
        "drupal/views_data_export": "^1.3",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/webform": "^6.2@beta",
        "drupal/workbench": "^1.4",
        "drupal/workbench_access": "^2.0",
        "drupal/workbench_menu_access": "^2.0",
        "drush/drush": "^11.0",
        "mglaman/composer-drupal-lenient": "^1.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "thunder/thunder-distribution": "^7.0@STABLE",
        "vlucas/phpdotenv": "^2.4",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "require-dev": {
        "acquia/blt-drupal-check": "dev-master",
        "drupal/core-dev": "^10.0",
        "mikemadison13/blt-lando": "dev-main"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": ["load.environment.php"]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "enable-patching": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 2000,
        "allow-plugins": {
            "drupal/core-composer-scaffold": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "oomphinc/composer-installers-extender": true,
            "acquia/blt": true,
            "wikimedia/composer-merge-plugin": true,
            "mglaman/composer-drupal-lenient": true
        }
    },
    "drupal-lenient": {
        "allowed-list": ["drupal/superfish"]
    }
}
